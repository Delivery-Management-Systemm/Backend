{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MyDB": "YOUR_SERVER;Database=FMS;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "SmtpSettings": {
